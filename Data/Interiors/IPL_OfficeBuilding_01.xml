    <!-- ========== INTERIOR SECURITY SYSTEM ========== -->
    <InteriorSecurity>
        <!-- BOUNDARY VALIDATION -->
        <BoundaryValidation>
            <EnableCollisionChecks>true</EnableCollisionChecks>
            <ValidatePlayerEntry>true</ValidatePlayerEntry>
            <KickInvalidPlayers>true</KickInvalidPlayers>
            <LogSuspiciousActivity>true</LogSuspiciousActivity>
        </BoundaryValidation>
        
        <!-- TELEPORT DETECTION -->
        <TeleportDetection>
            <MaxAllowedDistance>20.0</MaxAllowedDistance>
            <TimeWindow>1.0</TimeWindow>
            <SuspiciousSpeedThreshold>50.0</SuspiciousSpeedThreshold>
            <Action>VALIDATE_ENTRY_POINT</Action>
        </TeleportDetection>
        
        <!-- NOCLIP DETECTION -->
        <NoClipDetection>
            <CheckCollisionMesh>true</CheckCollisionMesh>
            <RequiredCollisionTypes>
                <Type>WALL</Type>
                <Type>FLOOR</Type>
                <Type>CEILING</Type>
            </RequiredCollisionTypes>
            <Action>FORCE_VALID_POSITION</Action>
        </NoClipDetection>
        
        <!-- VALID PLAYER STATES -->
        <PlayerValidation>
            <RequiredEntryValidation>true</RequiredEntryValidation>
            <AllowedEntryMethods>
                <Method>DOOR_TRIGGER</Method>
                <Method>WINDOW_BREAK</Method>
                <Method>ELEVATOR</Method>
                <Method>MISSION_TELEPORT</Method>
                <Method>ADMIN_TELEPORT</Method>
            </AllowedEntryMethods>
            <ForbiddenEntryMethods>
                <Method>TELEPORT</Method>
                <Method>NOCLIP</Method>
                <Method>WALL_PHASE</Method>
            </ForbiddenEntryMethods>
            
            <!-- MISSION SYSTEM -->
            <MissionTeleportSystem>
                <EnableMissionTeleports>true</EnableMissionTeleports>
                <RequiresMissionID>true</RequiresMissionID>
                <TeleportCooldown>5.0</TeleportCooldown>
                <LogMissionTeleports>true</LogMissionTeleports>
                <ValidMissionTypes>
                    <Type>STORY_MISSION</Type>
                    <Type>SIDE_MISSION</Type>
                    <Type>EMERGENCY_RESPONSE</Type>
                    <Type>ADMIN_OVERRIDE</Type>
                </ValidMissionTypes>
            </MissionTeleportSystem>
            
            <!-- ADMIN SYSTEM -->
            <AdminOverrideSystem>
                <EnableAdminOverrides>true</EnableAdminOverrides>
                <AllowAdminNoclip>true</AllowAdminNoclip>
                <AllowAdminTeleport>true</AllowAdminTeleport>
                <LogAdminActions>true</LogAdminActions>
                <RequireAdminAuth>true</RequireAdminAuth>
            </AdminOverrideSystem>
        </PlayerValidation>
    </InteriorSecurity>

    <!-- ========== COLLISION VALIDATION SYSTEM ========== -->
    <CollisionValidation>
        <!-- MESH COLLISION CHECKS -->
        <MeshCollisionChecks>
            <EnablePlayerMeshCollision>true</EnablePlayerMeshCollision>
            <EnableVehicleMeshCollision>true</EnableVehicleMeshCollision>
            <EnableObjectMeshCollision>true</EnableObjectMeshCollision>
            
            <!-- COLLISION LAYERS -->
            <CollisionLayers>
                <Layer name="INTERIOR_WALLS" enabled="true" blocking="true"/>
                <Layer name="INTERIOR_FLOORS" enabled="true" blocking="true"/>
                <Layer name="INTERIOR_CEILINGS" enabled="true" blocking="true"/>
                <Layer name="FURNITURE" enabled="true" blocking="true"/>
                <Layer name="DOORS" enabled="true" blocking="false"/>
                <Layer name="WINDOWS" enabled="true" blocking="true"/>
                <Layer name="INVISIBLE_BARRIERS" enabled="true" blocking="true"/>
            </CollisionLayers>
        </MeshCollisionChecks>
        
        <!-- PENETRATION DETECTION -->
        <PenetrationDetection>
            <CheckWallPenetration>true</CheckWallPenetration>
            <CheckFloorPenetration>true</CheckFloorPenetration>
            <CheckCeilingPenetration>true</CheckCeilingPenetration>
            <MaxAllowedPenetration>0.5</MaxAllowedPenetration>
            <PenetrationAction>FORCE_CORRECTION</PenetrationAction>
        </PenetrationDetection>
        
        <!-- POSITION VALIDATION -->
        <PositionValidation>
            <ValidateOnEntry>true</ValidateOnEntry>
            <ValidateOnMovement>true</ValidateOnMovement>
            <ValidateOnTeleport>true</ValidateOnTeleport>
            <ContinuousValidation>true</ContinuousValidation>
            <ValidationFrequency>0.1</ValidationFrequency>
        </PositionValidation>
        
        <!-- CORRECTION ACTIONS -->
        <CorrectionActions>
            <InvalidPositionAction>MOVE_TO_NEAREST_VALID</InvalidPositionAction>
            <WallPenetrationAction>PUSH_TO_SURFACE</WallPenetrationAction>
            <FloorPenetrationAction>RAISE_TO_FLOOR</FloorPenetrationAction>
            <CeilingPenetrationAction>LOWER_TO_CEILING</CeilingPenetrationAction>
            <TeleportViolationAction>RETURN_TO_ENTRY_POINT</TeleportViolationAction>
        </CorrectionActions>
        
        <!-- EMERGENCY FALLBACKS -->
        <EmergencyFallbacks>
            <InvalidPositionFallback>
                <Position x="0.0" y="-45.0" z="2.0"/>
                <Description>Main entrance fallback</Description>
            </InvalidPositionFallback>
            <ExtremeViolationFallback>
                <Position x="148.0" y="195.0" z="2.0"/>
                <Description>Exterior position fallback</Description>
                <ForceExterior>true</ForceExterior>
            </ExtremeViolationFallback>
        </EmergencyFallbacks>
    </CollisionValidation>

    <!-- ========== DOOR INTERACTION SYSTEM ========== -->
    <DoorInteractionSystem>
        <!-- DOOR TRIGGERS -->
        <DoorTriggers>
            <Trigger>
                <Name>main_door_trigger</Name>
                <Position x="148.0" y="195.0" z="2.0"/>
                <TriggerRadius>3.0</TriggerRadius>
                <RequiredInteraction>USE_KEY</RequiredInteraction>
                <InteractionPrompt>Press [E] to enter building</InteractionPrompt>
                <ValidatesEntry>true</ValidatesEntry>
                <LogsEntry>true</LogsEntry>
                <ConnectedEntryPoint>main_entrance</ConnectedEntryPoint>
