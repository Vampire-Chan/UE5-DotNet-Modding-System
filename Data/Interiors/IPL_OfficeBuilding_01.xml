                <LogsEntry>true</LogsEntry>
                <ConnectedEntryPoint>parking_garage</ConnectedEntryPoint>
                <AllowsVehicles>true</AllowsVehicles>
            </Trigger>
        </DoorTriggers>
        
        <!-- ENTRY VALIDATION -->
        <EntryValidation>
            <RequireValidDoorTrigger>true</RequireValidDoorTrigger>
            <LogAllEntryAttempts>true</LogAllEntryAttempts>
            <BlockInvalidEntries>true</BlockInvalidEntries>
            <KickRepeatedViolators>true</KickRepeatedViolators>
            <MaxViolationsBeforeKick>3</MaxViolationsBeforeKick>
        </EntryValidation>
        
        <!-- EXIT VALIDATION -->
        <ExitValidation>
            <RequireValidExitTrigger>true</RequireValidExitTrigger>
            <LogAllExitAttempts>true</LogAllExitAttempts>
            <BlockInvalidExits>false</BlockInvalidExits>
            <AllowEmergencyExits>true</AllowEmergencyExits>
        </ExitValidation>
    </DoorInteractionSystem>

