                <InteractionPrompt>Press [E] to enter side door</InteractionPrompt>
                <ValidatesEntry>true</ValidatesEntry>
                <LogsEntry>true</LogsEntry>
                <ConnectedEntryPoint>side_entrance</ConnectedEntryPoint>
            </Trigger>
            
            <Trigger>
                <Name>garage_door_trigger</Name>
                <Position x="140.0" y="210.0" z="-8.0"/>
                <TriggerRadius>5.0</TriggerRadius>
                <RequiredInteraction>PROXIMITY</RequiredInteraction>
                <InteractionPrompt>Garage door opening...</InteractionPrompt>
