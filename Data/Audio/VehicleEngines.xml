<?xml version="1.0" encoding="UTF-8"?>
<VehicleEngines>
    <!-- ========== UE5 VEHICLE ENGINE AUDIO SYSTEM ========== -->
    <!-- Pure C++ with TMap<FName, FVehicleEngineData> structures -->
    <!-- Connected to Data/Vehicles/Vehicles.xml via EngineAudioID -->

    <!-- ========== CARS ========== -->
    <VehicleEngineType id="ENGINE_COMPACT">
        <EngineSounds>
            <SoundEntry key="EngineStart" asset="/Game/Audio/Vehicles/Engines/Compact/CompactStart"/>
            <SoundEntry key="EngineStop" asset="/Game/Audio/Vehicles/Engines/Compact/CompactStop"/>
            <SoundEntry key="IdleLoop" asset="/Game/Audio/Vehicles/Engines/Compact/CompactIdle"/>
